{
  "routers": [
    {
      "url": "/health",
      "method": "GET",
      "api": "health"
    },
    {
      "url": "/user/signup",
      "method": "POST",
      "api": "signup"
    },
    {
      "url": "/user/login",
      "method": "POST",
      "api": "login"
    },
    {
      "url": "/user/logout",
      "method": "GET",
      "api": "logout"
    },
    {
      "url": "/user/info/:name",
      "method": "GET",
      "api": "showinfo",
      "middleware": "userRequired"
    }
  ]
}

