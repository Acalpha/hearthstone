    },
    {
      "url": "/user/favorites/:userId",
      "method": "GET",
      "api": "showFavorites",
      "middleware": "userRequired"
    },
    {
      "url": "/card/sync/:version",
      "method": "GET",
      "api": "syncCards"
    },
    {
      "url": "/comment/star/:commentId",
      "method": "GET",
      "api": "star"
    },
    {
      "url": "/comment/create",
      "method": "POST",
      "api": "createComment"
    }
