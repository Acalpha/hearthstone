{
  "routers": [
    {
      "url": "/health",
      "method": "GET",
      "api": "health"
    },
    {
      "url": "/user/signup",
      "method": "POST",
      "api": "signup"
    },
    {
      "url": "/user/login",
      "method": "POST",
      "api": "login"
    },
    {
      "url": "/user/logout",
      "method": "GET",
      "api": "logout"
    },
    {
      "url": "/user/info/:userId",
      "method": "GET",
      "api": "showinfo",
      "middleware": "userRequired"
    },
    {
      "url": "/user/update",
      "method": "PUT",
      "api": "updateUser",
      "middleware": "userRequired"
    },
    {
      "url": "/user/:userId/card/:cardId",
      "method": "GET",
      "api": "favorite",
      "middleware": "userRequired"
    },
    {
      "url": "/user/favorite/:userId",
      "method": "GET",
      "api": "showFavorites",
      "middleware": "userRequired"
    },
    {
      "url": "/comment/show/:cardId",
      "method": "GET",
      "api": "showComments"
    },
    {
      "url": "/comment/star",
      "method": "PUT",
      "api": "star"
    },
    {
      "url": "/comment/create",
      "method": "POST",
      "api": "createComment"
    },
    {
      "url": "/card/import",
      "method": "POST",
      "api": "importCards"
    },
    {
      "url": "/card/sync/:version",
      "method": "GET",
      "api": "syncCards"
    }
  ]
}

